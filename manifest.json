{
  "manifest_version": 2,
  "name": "ShortURL https://url.joor.net (pygmy)",
  "version": "0.3.1",
    "description": "pygmy shorturl Chrome extension for https://url.joor.net",
  "icons": {
    "16": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Create short URLs",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "clipboardWrite",
    "https://api.url.joor.net/"
  ]
}